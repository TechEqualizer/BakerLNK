{
  "extends": "./.eslintrc.cjs",
  "rules": {
    "no-console": ["error", { "allow": ["warn", "error"] }],
    "no-debugger": "error",
    "no-unused-vars": ["error", { "argsIgnorePattern": "^_" }],
    "react/prop-types": "off",
    "react/no-unescaped-entities": "off"
  },
  "env": {
    "node": true,
    "browser": true,
    "es2020": true
  },
  "globals": {
    "__dirname": "readonly",
    "module": "readonly",
    "require": "readonly"
  }
}